propcorrect <- aggregate(correct~subid, data=d.listeners, mean)
reject <- propcorrect[propcorrect$correct < .8,]
for (i in reject$subid) {
d.listeners<-d.listeners[d.listeners$subid != i,]
}
#Remove incorrect
