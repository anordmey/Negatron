@article{hamlin2007,
	annote = {In a series of studies, Hamlin et al. use  explicit measures of children's preferences as well as implicit measures of their eye gaze to gain information about how children react to a novel agent interacting in either a positive (helping) or negative (hindering) way with an unknown third party.  In their first study, children viewed an abstract character (portrayed as a shape with eyeballs) attempting to get up a hill. Children then viewed alternating scenes in which a second character attempted to help the climber get up the hill, or a second character pushed the climber down the hill.  After being habituated to these scenes, children were given the choice to choose one of the characters, either the helper or the hinderer.  Children were significantly more likely to choose the helping character.  When children viewed scenes of the climber approaching either the helper or the hinderer, 10-month-olds looked more when the climber approached the hinderer, but 6-month-olds showed no preference.  A second study controlled for the possibility that children may simply prefer upwards motions to downwards ones, demonstrating that this is not the case -- it is something about the fact that the climber is a social agent that elicits this effect.  A third study introduced a neutral character and found that infants preferred the helper over the neutral character, but preferred the neutral character over the hinderer.  This study suggests that infants' are driven both by positive feelings for the helper, as well as an aversion to the hinderer.   Interestingly, in this third study, neither 10-month-old nor 6-month-old infants showed differential looking when viewing the climber approach the neutral agent v. the helper, or the hinderer v. the neutral agent.  },
	author = {Hamlin, J.K. and Wynn, K. and Bloom, P.},
	date-added = {2012-05-17 17:30:53 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	journal = {Nature},
	keywords = {theory of mind, social development, eye tracking},
	number = {7169},
	pages = {557--559},
	publisher = {Nature Publishing Group},
	title = {Social evaluation by preverbal infants},
	volume = {450},
	year = {2007},
	bdsk-file-1 = {}}

@article{goodman2011,
	abstract = {The very early appearance of abstract knowledge is often taken as evidence for innateness. We explore the relative learning speeds of abstract and specific knowledge within a Bayesian framework and the role for innate structure. We focus on knowledge about causality, seen as a domain-general intuitive theory, and ask whether this knowledge can be learned from co-occurrence of events. We begin by phrasing the causal Bayes nets theory of causality and a range of alternatives in a logical language for relational theories. This allows us to explore simultaneous inductive learning of an abstract theory of causality and a causal model for each of several causal systems. We find that the correct theory of causality can be learned relatively quickly, often becoming available before specific causal theories have been learned---an effect we term the blessing of abstraction. We then explore the effect of providing a variety of auxiliary evidence and find that a collection of simple perceptual input analyzers can help to bootstrap abstract knowledge. Together, these results suggest that the most efficient route to causal knowledge may be to build in not an abstract notion of causality but a powerful inductive learning mechanism and a variety of perceptual supports. While these results are purely computational, they have implications for cognitive development, which we explore in the conclusion. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
	author = {Goodman, N.D. and Ullman, T.D. and Tenenbaum, J.B.},
	date-added = {2012-05-17 17:21:31 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	journal = {Psychological review},
	keywords = {causal learning, modeling},
	number = {1},
	pages = {110},
	publisher = {American Psychological Association},
	title = {Learning a theory of causality.},
	volume = {118},
	year = {2011},
	bdsk-file-1 = {}}

@article{ganea2007,
	abstract = {One of the most distinctive characteristics of humans is the capacity to learn from what other people tell them. Often new information is provided about an entity that is not present, requiring incorporation of that information into one's mental representation of the absent object. Here we present evidence regarding the emergence of this vital ability. Nineteen- and 22-month-old infants first learned a name for a toy and later were told that the toy had undergone a change in state (it had become wet) while out of view. The 22-month-olds (but not the 19-month-olds) subsequently identified the toy solely on the basis of the property that they were told about but had never seen. Thus, before the end of their 2nd year, infants can use verbal information to update their representation of an absent object. This developmental advance inaugurates a uniquely human and immensely powerful form of learning about the world.},
	author = {Ganea, P.A. and Shutts, K. and Spelke, E.S. and DeLoache, J.S.},
	date-added = {2012-05-17 17:18:32 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
