@book{bloom1970,
	address = {New York, NY},
	annote = {Conducted a longitudinal study of three children, Kathryn (21-24 months), Eric(19-27 months), and Gia(19-27 months).  Found many instances of external negation in these children's speech, and interpreted these utterances as anaphoric, i.e. \cite{bloom1970, bloom1993}: \begin{center}[The child cannot connect two toy trains]\\No [I can't/don't want to,] Lois do it. \end{center}  
In addition, Bloom catalogued the types of negative utterances that the three children in her study produced, identifying three basic types that developed in the same order in each child.  The first type of negation to emerge was nonexistence, in which negation is used the refer to an object that is not perceptually present.  The second type, which emerged shortly after nonexistence, was rejection, in which negation is used to refuse or turn away some object that is present in the context.  The final type of negation to emerge in Bloom's data was denial, in which the child forms a negative utterance to make a statement about something that is false in the world around them.  Bloom's analysis was one of the first to classify children's negations into different categories in this way.  \cite{bloom1970, bloom1993}},
	author = {Bloom, L.},
	date-added = {2011-12-05 02:05:58 +0000},
	date-modified = {2013-09-13 20:02:44 +0000},
	keywords = {negation, language acquisition},
	publisher = {MIT Research Monograph, No 59.},
	read = {0},
	school = {Columbia University},
	title = {Language development: Form and function in Emerging grammars.},
	year = {1970}}

@article{wason1965,
	annote = {Designed a study to examine whether pragmatic constraints such as the context that a negation was presented in would affect how participants responded.  Wason presented subjects with a string of 8 red or blue dots, 7 of which were one color and 1 of which was the remaining color.  In the Exceptionality group, the string of dots was numbered 1-8, while in the Ratio group, the string of dots was not numbered.  Subjects were asked to produce a sentence describing the dots in their own words, and then were asked to evaluate the truth or falsity of sentences such as ``circle number 7 is red'' /``circle number 4 is not blue'' (Exceptionality group) or ``exactly 7 circles are red'/``exactly 1 circle is not blue'' (Ratio group).  Participants were significantly slower to respond to negative statements about dissimilar items in the Ratio group (i.e. ``exactly 1 circle is not blue'' in context where 7 of the circles were blue) than in the Exceptionality group (i.e. ``circle number 4 is not blue'' in contexts were the rest of the circles were blue).  In addition, in the Ratio group participants tended to mention the dissimilar dot last in their descriptions of the stimuli, i.e. ``seven circles are blue and one is red'', whereas in the Exceptionality group participants were more likely to describe the unusual dot in terms of a residual class, i.e. ``circle number 4 is red and the rest are blue''.  The differences in how participants described the dots, combined with the different reaction times seen in response to the dissimilar negative sentences, suggests that the processing of negative sentences in which some expected characteristic is denied is facilitated by the presence of a context that sets up an exceptional and residual class.},
	author = {Wason, P.C.},
	date-added = {2011-12-04 01:36:17 +0000},
	date-modified = {2014-01-31 00:56:57 +0000},
	journal = {Journal of Verbal Learning and Verbal Behavior},
	keywords = {negation, sentence verification},
	number = {1},
	pages = {7--11},
	publisher = {Elsevier Science},
	title = {The contexts of plausible denial.},
	volume = {4},
	year = {1965},
	bdsk-file-1 = {}}

