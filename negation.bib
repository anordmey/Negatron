@inproceedings{klima1966,
	address = {Edinburgh},
	annote = {One of the first studies to examine children's production of negation.  Analyzed natural speech samples from three children (Brown corpus: Adam, Eve, and Sarah), ages 18, 26, and 27 months at the beginning of the study.  They outlined three stages of negation that children go through before they begin producing adult-like negation.  The first of these stages, often referred to as "external negation", is a period marked by a tendency for young children to place the negative element at the front of the sentence, i.e. \begin{center} No the sun is shining \cite{klima1966} \end{center} In stage two children's negatives begin to follow the structure Nominal - Aux(neg) - Predicate/Main Verb, where children's negations generally take the form of can't, don't, and not.  They found that at this stage, children were only producing auxiliary verbs in negative forms (not questions or declarative utterances).  At stage 3, children's negations became even more adult-like, with the basic structure Nominal - Auxiliary - Predicate/Main Verb.  At this point children are producing auxiliary verbs in other constructions as well, and there is also evidence for transformations in children's constructions, such as optional `be' deletion and `do' deletion.  These researchers argued that what appear to be errors in children's speech actually represented a systematic child "grammar", with it's own set of rules.},
	author = {Klima, E.S. and Bellugi, U.},
	booktitle = {Psycholinguistic Papers},
	date-added = {2011-12-02 00:10:27 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	editor = {J. Lyons, R.J. Wales},
	keywords = {negation},
	pages = {183-208},
	publisher = {Edinburgh University Press},
	title = {Syntactic regularities in the speech of children},
	volume = {3765},
	year = {1966},
	bdsk-file-1 = {}}

@article{pea1982,
	annote = {Conducted a study which examined 2 -- 3-year-old children's spontaneous denials.  Children were shown either an object with a specific label or description, or a doll performing an action.  The experimenters then said sentences (for example,``that's the ball'', ``she's sitting'', or ``that's the yellow one'', or negative versions of similar sentences) that were either congruous with the scene or incongruous with the scene.  Results showed that while 30 -- 36 month olds responded to false affirmatives with a denial ``no'' significantly more than they did with true affirmatives, only some 24-month-olds produced ``no'' responses to false affirmatives, and 18-month-olds made almost no responses at all.  This study suggested that the spontaneous production of denial negations emerges somewhere around 2 years of age.},
	author = {Pea, R.D.},
	date-added = {2011-12-02 00:10:03 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	journal = {Journal of Child Language},
	keywords = {negation},
	number = {3},
	pages = {597-626},
	publisher = {Cambridge Univ Press},
	title = {Origins of verbal logic: Spontaneous denials by two-and three-year olds},
	volume = {9},
	year = {1982},
	bdsk-file-1 = {}}

@incollection{pea1980,
	address = {New York, NY},
	annote = {Conducted an analysis of the types of negation seen in children's early speech.  Pea's study examined 6 children from 8 to 21 months, and examined both gestural and linguistic negations produced by these children over the course of a year.  Pea identified five different categories.  Rejection was the first to emerge in Pea's sample (gestures: 10-14 months, utterances: 12-15 months).  Disappearance (negations referring to an object that has recently disappeared or an action that has recently ceased), emerged consecutively with spoken rejection (utterances: 13-15 months), followed by self-prohibition (negations used in situations where the child is engaging in an action or interacting with an object that has been forbidden in the past; gestures: 11-16 months, utterances: 14-21 months), and unfulfilled expectation (negations used in cases where an object is not in its expected location or an expected action cannot occur; gestures: 14 months, utterances: 16-18 months).  The last to develop was Truth-Functional negation, or negations used by the child in situations in which the child is engaging in an action or interacting with an object that has been forbidden in the past (utterances: 19-23 months). },
	author = {Pea, R.D.},
	booktitle = {The Social Foundations of Language and Thought},
	date-added = {2011-12-02 00:09:42 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	editor = {Olson, D.R.},
	journal = {The Social Foundations of Language and thought},
	keywords = {negation},
	pages = {156--186},
