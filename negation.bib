@article{hermer-vazquez1999,
	annote = {Provides evidence for the idea that although non-linguistic creatures or pre-linguistic children can use single, separate modules for simple reasoning, language is required to link information from multiple modules.  Examined specifically whether language is necessary to integrate information from a `landmark' module and a `geometric' module during a search task.  In this study, researchers examined adults' ability to reorient themselves in a plain white rectangular room compared to a rectangular room with one wall painted blue.  In the room with the blue wall, adults combined geometric information with information about the colored wall to locate the precise position of a hidden object e.g. ``to the left of the blue wall''.  In the room with only white walls, adults performed in a manner similar to children and rats (Hermer and Spelke, 1996, as cited in Hermer-Vazquez, Spelke, \& Katsnelson, 1999), searching equally in the correct corner as well as the geometric opposite corner for a hidden object.  However, in further studies, Hermer-Vazquez et al. (1999) found that when engaged in a verbal-shadowing task, in which participants listened to a story through headphones and repeated what they heard out loud simultaneously, adults performance was reduced to that of children and rats'.  When adults performed the same task with a rhythmic-interference task, they performed on par with non-shadowers.  This result suggests that when their linguistic capabilities are suppressed, adults are incapable of using the conjunction of geographic and non-geographic information to find an object in a searching task, providing support for Hinzen's theory that language is a necessary component in the combination of simple concepts into complex ones.  },
	author = {Hermer-Vazquez, L. and Spelke, E.S. and Katsnelson, A.S.},
	date-added = {2011-12-08 06:02:18 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	journal = {Cognitive psychology},
	keywords = {language & thought, interference, spatial orientation},
	number = {1},
	pages = {3--36},
	publisher = {Elsevier},
	title = {Sources of flexibility in human cognition: Dual-task studies of space and language},
	volume = {39},
	year = {1999},
	bdsk-file-1 = {}}

@article{schneider2010,
	annote = {Observation of 25 great apes, making note of any head shaking behaviors that the apes exhibited.  Head gestures were noted in bonobos and chimpanzees, but only bonobos displayed head shaking gestures similar to a human gesture for ``no''.  After observing the instances of head shaking behavior in bonobos, the researchers determined that 13 out of 49 head shaking gestures performed by the bonobos were used to indicate unwillingness to participate in an action.  However, while this study does suggest that bonobos may have a symbolic gesture that carries the meaning ``no'', it is more likely that the kind of negation that is being expressed here is rejection, rather than denial negation.  Thus, it is unclear whether this capability would require representational thought at all.  It is reasonable to assume that a non-verbal animal such as a bonobo would be capable of having a simple concept of ``negation'', such as the ability to refuse something, but it remains unclear whether it is possible to have an understanding of denial, and a general concept of negation, without the structures of language. },
