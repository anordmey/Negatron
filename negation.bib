@article{bergelson2013,
	abstract = {In this paper, Bergelson & Swingley examined children's comprehension of abstract words such as ``eat'' and ``all gone''.  These phrases generally involve referents that are not perceptually present or that are perceptually diverse, and therefore require more than simple associative cues for word learning.  The authors suggest that learning these words requires a more complex understanding of the role of intentionality in language.  To test comprehension of abstract words, children saw fourteen videos of different actions presented in yoked pairs.  While the videos were playing, parents listened to a utterance in headphones and repeated it for their child (e.g. ``look! all gone!  All gone!'').  Difference scores were computed to determine the increase in target looking following the parent's utterance.  Infants showed some comprehension at 10 months, and even great comprehension at 14 months.  This suggests that abstract words take longer to learn than concrete words, which elicit some comprehension as young as 6 months; however, comprehension can still be seen very early.  },
	author = {Bergelson, Elika and Swingley, Daniel},
	date-added = {2013-07-10 21:07:04 +0000},
	date-modified = {2013-07-19 18:44:04 +0000},
