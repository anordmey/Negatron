@mastersthesis{gilkerson2002,
	annote = {Examined 14 -- 25-month-old children's comprehension of denials in a preferential looking paradigm.  Children were shown two pictures, and then heard either an affirmative sentence matching one of the pictures, or a sentence negating the scene of the same picture.  Children were slightly more likely to look to the matching picture when they heard an affirmative sentence than when they heard a negation.  That is, if a child saw pictures of a girl sitting and a girl sleeping, that child was more likely to look to the picture of the girl sleeping if she heard ``The girl is sleeping'' than if she heard ``The girl is not sleeping'', providing some evidence that children were correctly comprehending the denial negations.  However, the differences seen in children's looking preferences were not statistically significant.  In addition, only two children in this study were below 19 months, and age was not examined in the analysis, making it difficult to determine precisely when the comprehension of negation emerges.  },
	author = {Gilkerson, J. and Hyams, N. and Curtiss, S.},
	date-added = {2011-12-02 00:13:15 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	keywords = {negation, eye tracking},
	school = {University of Maryland, College Park},
	title = {`No I understand negation': a preferential looking paradigm study of early knowledge of sentential and anaphoric negation.},
	year = {2002},
	bdsk-file-1 = {}}

@phdthesis{loder2006,
	annote = {Tested comprehension in a preferential looking paradigm.  Focused on finding a more precise age at which children could comprehend negation.  In this study, 18-month-old infants were shown pairs of pictures in which the same agent performed two different actions on separate screens (i.e. a bug flying and a bug jumping).  During the presentation of the pictures, the child heard ``Look, the bug's not jumping!'' several times.  Results were inconclusive, with children looking significantly more to the correct picture in some trials but not in others.  This suggests that children are not able to comprehend denial negation at 18 months.   },
	author = {Loder, L.S.},
	date-added = {2011-12-02 00:11:57 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	keywords = {negation, eye tracking},
	title = {Early Understanding of Negation: The Word" Not"},
	year = {2006},
	bdsk-file-1 = {}}

