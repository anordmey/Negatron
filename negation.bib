@article{carpenter1975,
	annote = {A sentence verification task, in which participants evaluate the truth or falsity of a sentence paired with a simple picture, to evaluate differences in how adults process negative sentences as opposed to affirmative sentences. Presented participants with sentences such as ``It's true that the dots are red'', ``it's true that the dots aren't red'', ``It isn't true that the dots are red'', and ``it isn't true that the dots aren't read''.  The results of this study also indicated that adults are slower to respond to negative sentences than affirmative sentences, and are faster to respond to true sentences when the sentence is affirmative, but faster to respond to false sentences when the sentence is negative.  In addition, Carpenter and Just found that participants were overall slower to respond to denial, i.e. ``It isn't true that x''.  These studies indicate that there is something about the representation of negation that causes individuals to process negative sentences slower than positive/affirmative sentences },
	author = {Carpenter, P.A. and Just, M.A.},
	date-added = {2011-12-05 02:07:02 +0000},
