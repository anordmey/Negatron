@article{choi1988,
	annote = {Cross-linguistic, longitudinal observation of 11 children (2 English-speaking, 4 Korean-speaking, and 5 French-speaking), ages 1;7-3;4.  Examined the emergence between forms and functions, defining functions as different ways children use negation, and categories as defined by the emergence of forms that are specific to a certain function.  Distinguished between independent forms, which use a single term to negate a proposition in the preceding context (e.g. `no'), and syntactic forms, which encompasses both general negation (a negative element + a predicate to negate a proposition) and syntactic negation (forms used in specific contexts).  Also distinguished between nonlinguistic differences between categories, i.e.  differences between the contexts and types of propositions that negation occurs in, as well as linguistic differences between categories, in which different linguistic forms are used for different categories.  Identified 9 categories:  nonexistence (an expected object is not present), rejections (negation of another's action), prohibition (negation of one's own action), failure (an expected action is not occurring), denial (negation of a linguistically represented proposition), inability (negation of a physical ability, e.g. `I can't'), epistemic negation (negation of knowledge, e.g. `I don't know'), inferential negation (negation of inferred beliefs of others), and normative negation (negation that expresses a general violation of expectations).  Defined three developmental phases of negation.  In phase 1, nonexistence, prohibition, rejection, and failure emerge.  In phase 2, denial, inability, and epistemic negation emerge.  In phase three, normative and inferential negation emerge.  Nonexistence, rejection, prohibition, and failure all had primarily nonlinguistic differences (though nonexistence did emerge with specific forms), whereas the others were differentiated using specific linguistic forms.  This was taken as evidence that categories such as nonexistence, rejection, prohibition, and failure are preceded by the development of cognitive concepts which then map onto linguistic forms.  However, in phases 2 and 3, children seem to be using language to develop more complex concepts about negation.  Identified three different relationships between form and function: old forms express new functions, new forms express new functions, and new forms differentiate old functions.},
	author = {Choi, S.},
	date-added = {2011-12-05 09:03:50 +0000},
	date-modified = {2013-09-13 19:39:41 +0000},
	journal = {Journal of Child Language},
	keywords = {negation, crosslinguistic},
	pages = {517--531},
	publisher = {Cambridge University Press},
	title = {The semantic development of negation: A cross-linguistic longitudinal study},
	volume = {15},
	year = {1988},
	bdsk-file-1 = {}}

@article{levy2008,
	annote = {The difficulty of a word is equal to the surprisal of that word given its context.  Surprisal is the negative log of the conditional probability.  See paper for more details.  

The authors develop a probabilistic, expectation-based theory of syntactic comprehension.  Surprisal theory and probabilistic models make accurate predictions about how sentences are represented, and about the level of processing difficultly for specific words in a sentence.  Sentence comprehension is incremental - we start processing before we finish the sentence.  Expectations about upcoming words are implicit in the parsing of a sentence, rather than explicitly calculated.  

The predicted difficulty of a word w is equal to it's surprisal, i.e. the negative log-probability of w in a sentential context.  Surprisal mediates the relationship between structural representation and comprehension difficulty.  Surprisal values correspond to average reading times.  
},
	author = {Levy, R.},
	date-added = {2011-12-05 02:09:28 +0000},
	date-modified = {2014-01-31 00:56:09 +0000},
	journal = {Cognition},
	keywords = {surprisal, modeling, syntax, sentence processing},
	pages = {1126--1177},
	publisher = {Elsevier},
	title = {Expectation-based syntactic comprehension},
	volume = {106},
	year = {2008},
	bdsk-file-1 = {}}

