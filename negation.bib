@book{premack1976,
	annote = {David Premack conducted a longitudinal study in which he attempted to teach several chimpanzees, Sarah, Elizabeth, and Peony, a symbolic system based on plastic tokens that could be manipulated to form sentences.  One important aspect of this symbolic system was the inclusion of tokens for the concepts ``same'', ``different'', and ``not''.  Premack trained the chimps to use the symbols to form simple ``sentences'', such as: \begin{center} Apple SAME apple\\Apple DIFFERENT banana \end{center} The chimps were then trained to understand the negative token as negating an instruction: \begin{center} NO Sarah take apple \end{center} Sarah was also trained to respond YES or NO to questions such as: \begin{center} ? Apple SAME apple\\Correct response: YES\\?Apple SAME banana?\\Correct response: NO\end{center}Premack then tested the chimps on their response to novel sentences.  All of the chimps learned to inhibit their responses when they heard novel instructions with the negative particle NO (i.e. Sarah would take the cracker when she saw ``Sarah jam-cracker take'' but would not reach for any food when she saw ``No Sarah peanut butter-bread take'').  However, it is unclear how sophisticated this understanding was; it is possible that the chimps took ``no'' to mean ``don't move'' rather than ``don't perform this specific action''.   Sarah, the only chimp who was trained on answering Yes/No questions, had much more difficulty generalizing this knowledge to learn that that NO-DIFFERENT could be substituted for SAME.  Although Sarah was sometimes able to create the sentence ``Apple NO DIFFERENT Apple'' when the token for ``same'' was not present, her performance was riddled with errors.  Despite the fact that it is difficult to draw any concrete conclusions from a study with so few subjects and such varying results, Premack's work with chimpanzees raises a number of questions about the nature of animal thought and cognition.  If it is possible for a chimpanzee to form novel, albeit simple, sentences describing second order relations such as ``same'', ``different'', or ``not'', does this mean that the chimpanzee does in fact have an underlying system of propositional thought?  Or does this mean that teaching the chimpanzees a symbolic system actually provided them with simple structures that allowed them to construct mental representations in propositional form?  Although Premack's experiments provided an illuminating glimpse into the cognitive abilities of chimpanzees, there are still many unanswered questions.  
},
	author = {Premack, D.},
	date-added = {2011-12-08 05:43:54 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	keywords = {negation, animal study},
	publisher = {Lawrence Erlbaum Associates Hillsdale, New Jersey},
	title = {Intelligence in ape and man},
	year = {1976}}

@article{hummer1993,
	annote = {Conducted a study to examine the development of denial negation in children ages 1;1 to 2;7.  The study used an elicitation paradigm in which children were asked to respond either yes or no, rather than examining their spontaneous denials.  The researchers observed children while they read a picture book with their mothers.  During this activity, researchers asked children yes/no questions about the picture-book that they were engaged with.  This task required children to evaluate the truth or falsity of a proposition (embedded within the Y/N question) based on the facts that they were given by the picture book.  Hummer et al. interpreted ``no'' responses as denials, since denial negation requires one to evaluate the truth of a proposition in this way.  The researchers found that children were most likely to produce the denial ``no'' between 20 and 25 months, similar to the ages at which Pea found spontaneous denial negation emerging.  However, due to the fact that Hummer et al. found that these forms of ``no'' emerged rapidly, with no errors, the researchers rejected Pea's 1980 proposal that different types of negation follow different developmental paths.  Hummer et al. instead use their data as support for the continuity theory of negation development, arguing that children's understanding of denial ``no'' derives from their earlier understanding of negative terms.  },
	author = {Hummer, P. and Wimmer, H. and Antes, G.},
	date-added = {2011-12-08 05:39:39 +0000},
	date-modified = {2013-07-19 18:44:05 +0000},
	journal = {Journal of Child Language},
	keywords = {negation},
	pages = {607--607},
	publisher = {Cambridge Univ Press},
	title = {On the origins of denial negation},
	volume = {20},
	year = {1993},
	bdsk-file-1 = {}}

